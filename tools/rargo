CC="$R/r5/sdk/riscv64-linux-musl-cross/bin/riscv64-linux-musl-cc" CXX="$R/r5/sdk/riscv64-linux-musl-cross/bin/riscv64-linux-musl-c++" HOST_CC=/usr/bin/cc HOST_CXX=/usr/bin/c++ RUSTFLAGS="-C linker=$R/r5/sdk/riscv64-linux-musl-cross/bin/riscv64-linux-musl-ld -C target-feature="+crt-static"" HOST_AR="/usr/bin/ar" AR="$R/r5/sdk/riscv64-linux-musl-cross/bin/riscv64-linux-musl-ar" cargo +nightly build -Zbuild-std=std,panic_abort --target=riscv64gc-unknown-linux-musl